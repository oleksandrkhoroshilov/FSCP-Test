    {
        const str FeatureManagementWhatsNewPrintMgmtUtilsKey = '{351B7E53-9B9B-4EDC-97E7-295D14E77091}';
            
        if (DocConstant::PrintMgmtUtilsManualURL != '')
        {
            SystemNotificationsWhatsNewManager::AddWhatsNewWithActionLink(
                      FeatureManagementWhatsNewPrintMgmtUtilsKey,
                      "@DocentricAX4:PrintMgmtUtilsCalloutTitle",
                      "@DocentricAX4:PrintMgmtUtilsCalloutText",
