				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            #Task

            switch (counterType)
            {                
                case DocPrintMgmtCounterType::SettingsWithoutOverrides:
                    counterType = DocPrintMgmtCounterType::SettingsIncludingOverrides;
                    infoTextStr.text("@DocentricAX3:CountSettingsIncludingOverrides");
                    break;
                
                case DocPrintMgmtCounterType::SettingsIncludingOverrides:
                    counterType = DocPrintMgmtCounterType::Documents;
                    infoTextStr.text("@DocentricAX3:CountDocuments");
                    break;

                case DocPrintMgmtCounterType::Documents:
                    counterType = DocPrintMgmtCounterType::SettingsWithoutOverrides;
                    infoTextStr.text("@DocentricAX3:CountSettingsWithoutOverrides");
                    break;

                default:
                    // Do nothing
            }

            PrintMgmtAccountsPayableTile.forceRefresh();
            PrintMgmtAccountsReceivableTile.forceRefresh();
            PrintMgmtInventoryManagementTile.forceRefresh();
            PrintMgmtProjectManagementTile.forceRefresh();
            PrintMgmtTransportationManagementTile.forceRefresh();
            PrintMgmtWarehouseManagementTile.forceRefresh();

            element.task(#taskRefresh);
        }

]]></Source>
