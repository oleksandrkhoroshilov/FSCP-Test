			<Method>
				<Name>getDocumentCount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets the document count for the specified print management node type.
    /// </summary>
    /// <param name = "_module">Print management node type</param>
    /// <param name = "_cnt">Number of documents</param>
    /// <returns>Number of document types</returns>
    private str getDocumentCount(PrintMgmtNodeType _module, int64 _cnt)
    {
        str retCount;

        PrintMgmtSettings settings;
        PrintMgmtDocInstance docInstance;

        switch (counterType)
        {
            // Number of document types in a module equals the number of records
            // in the corresponding info table.
            case DocPrintMgmtCounterType::Documents:
                retCount = strFmt('%1', _cnt);
                break;
                        
            // Number of document types in a module for which at least one module-level setting exists.
            case DocPrintMgmtCounterType::DocumentsWithSettings:                
                int cnt = 0;
                while select DocumentType from docInstance      // we can't use count(DocumentType) because it is enum, so we must count them one by one
                    group by docInstance.DocumentType
                        where docInstance.NodeType == _module
                    exists join infoTable
                        where infoTable.DocumentType == docInstance.DocumentType
                            && infoTable.NodeType == _module
                {
                    cnt++;
                }     
                retCount = strFmt('%1', cnt);
                break;
                
            // Simply count all existing settings for this module,
            // and don't filter by the node type. It will return both the module-level
            // settings and all overrides.
            case DocPrintMgmtCounterType::SettingsIncludingOverrides:
                select count(RecId) from settings
                    exists join docInstance
                        where docInstance.RecId == settings.ParentId
                    exists join infoTable
                        where infoTable.DocumentType == docInstance.DocumentType
                            && infoTable.NodeType ==_module;
                retCount = strFmt('%1', settings.RecId);
                break;
                
            // Initial counter type: Print management settings
            // Count all module-level settings for this module (Original, Copy, special settings).
            case DocPrintMgmtCounterType::SettingsWithoutOverrides:
                select count(RecId) from settings
                    exists join docInstance
                        where docInstance.RecId == settings.ParentId
                    exists join infoTable
                        where infoTable.DocumentType == docInstance.DocumentType
                            && infoTable.NodeType == docInstance.NodeType   // filter only the module-level settings
                            && infoTable.NodeType ==_module;
                retCount = strFmt('%1', settings.RecId);
                break;

            default:
                retCount = 'N/A';
        }

        return retCount;
    }

]]></Source>
