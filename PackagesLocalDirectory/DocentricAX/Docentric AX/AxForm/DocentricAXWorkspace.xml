				<Name>initPrintMgmtDocumentTypes</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the print management document types.
    /// </summary>
    private void initPrintMgmtDocumentTypes()
    {
        infoTable = DocPrintMgmtInfoTmp::fillTableForAllModules(true);  // Get info for current company

        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::Purch;
        cntPurchDocs = infoTable.RecId;
        
        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::Sales;
        cntSalesDocs = infoTable.RecId;
        
        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::Project;
        cntProjectDocs = infoTable.RecId;
        
        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::Invent;
        cntInventDocs = infoTable.RecId;
        
        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::WHS;
        cntWHSDocs = infoTable.RecId;
        
        select count(RecId) from infoTable where infoTable.NodeType == PrintMgmtNodeType::TMS;
        cntTMSDocs = infoTable.RecId;
    }

]]></Source>
