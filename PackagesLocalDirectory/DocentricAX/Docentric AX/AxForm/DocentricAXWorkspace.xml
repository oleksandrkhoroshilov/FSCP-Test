			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the form.
    /// </summary>
    public void init()
    {
        super();

        DocUpgradeToFullVersion.visible(DocLicenseParameters::find().LicenseType == DocReportParamLicenseType::FreeEdition);

        // Fetch the document types for each Print Management module
        this.initPrintMgmtDocumentTypes();

        // Initialize the counters to show the number of settings that will be found
        // on a module-level print management, because the click on a tile
        // will take the user to that form.
        counterType = DocPrintMgmtCounterType::SettingsWithoutOverrides;
        infoTextStr.text(enum2Str(counterType));        

        // Show/Hide Print Management per module buttons
        Set nodeTypesAllowedForUser = DocPrintMgmtHelper::getNodeTypesAllowedForUser();
        PrintMgmtAccountsPayableTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::Purch));
        PrintMgmtAccountsReceivableTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::Sales));
        PrintMgmtInventoryManagementTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::Invent));
        PrintMgmtProjectManagementTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::Project));
        PrintMgmtTransportationManagementTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::TMS));
        PrintMgmtWarehouseManagementTile.visible(nodeTypesAllowedForUser.in(PrintMgmtNodeType::WHS));

        // If no module-related menu item is visible, then also hide the whole group
        PrintMgmtGroup.visible(
            PrintMgmtAccountsPayableTile.visible() ||
            PrintMgmtAccountsReceivableTile.visible() ||
            PrintMgmtInventoryManagementTile.visible() ||
            PrintMgmtProjectManagementTile.visible() ||
            PrintMgmtTransportationManagementTile.visible() ||
            PrintMgmtWarehouseManagementTile.visible()
            );

        // Feature callouts
        element.addWhatsNewNotifications();
    }

