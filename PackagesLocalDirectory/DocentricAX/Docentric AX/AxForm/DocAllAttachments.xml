			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the form.
    /// </summary>
    public void init()
    {
        #SysSystemDefinedButtons

        // Apply the filter to the DocuRef data source if the current user has the DocentricAXAllAttachmentsUser privilege.
        // When the filter is applied, the current user only sees attachments to which he has access.
        if (DocGlobalHelper::hasUserSecurityPrivilege(privilegeStr(DocentricAXAllAttachmentsUser)) && !DocDocuRefHelper::isAttachmentAdmin())
        {
            applyFilter = true;
        }

        super();

        // Toggles preview pane based on last user setting.
        DocDocuRefHelper::initDocumentPreviewBasedOnSysLastValue(this);

        if (applyFilter)
        {
            // Populate the temporary table used for filtering the DocuRef data source
            DocAllAttachmentsTmp.linkPhysicalTableInstance(DocAllAttachmentsTmp::populateDocuRefForCurrentUser());
        }

        // Set form caption
        element.design().caption(strFmt("@DocentricAX4:AllAttachmentsByCompany", strUpr(curExt())));
        
        // Hide Attach button
        FormCommandButtonControl button = this.control(this.controlId(#SystemDefinedAttachButton)) as FormCommandButtonControl;
        if (button)
        {
            button.visible(false);
        }

        // The field IsGlobalAttachment_DC can be updated only in the DocReportGlobalAttachmentDetail form
        DocuRef_ds.object(fieldNum(DocuRef, IsGlobalAttachment_DC)).visible(false);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setDocuViewBlockClearFlag</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the blockClear_DC flag to prevent clearing the data in the DocuView form.
    /// </summary>
    private void setDocuViewBlockClearFlag()
    {
        // Prevent clearing the data in the DocuView form
        if (Docu::Instance() && Docu::Instance().docuView() && Docu::Instance().docuView().object())
        {
            Docu::Instance().docuView().object().parmBlockClear_DC(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    /// <summary>
    /// Called when the form is closed.
    /// </summary>
    public void close()
    {
        // Clear global cache for owner 'DocAllAttachments'.
        // It is set in the allowViewOrEditDocuRef method in the DocDocuRefHelper class
        // and stores the AllowView and AllowEdit flags for each selected DocuRef record.
        // Use SGC and not SGOC because we want the scope to be in the current user session.
        // Owner - DocAllAttachments, Key - DocuRef.RecId, Value - [AllowView (true/false), AllowEdit (true/false)].
        SysGlobalCache globalCache = ClassFactory.globalCache();
        globalCache.clear(formStr(DocAllAttachments));

        super();
    }

]]></Source>
