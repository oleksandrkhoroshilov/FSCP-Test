								<Name>jumpRef</Name>
								<Source><![CDATA[
            /// <summary>
            /// Opens a referenced form that contains data for the field.
            /// </summary>
            public void jumpRef()
            {
                if (!DocReportPdfSignatureSettings.CompanyId)
                {
                    DocGlobalHelper::handleWarning("@DocentricAX2:SelectCompanyFirst");
                }

                CompanyId curCompanyId = curExt();
                appl.setDefaultCompany(DocReportPdfSignatureSettings.CompanyId, false);

                try
                {
                    Args args = new Args();
                    args.lookupRecord(DocElectronicSignatureCertificate::find(DocReportPdfSignatureSettings.CertificateId));
                    args.caller(element);

                    FormRun formRun = new MenuFunction(menuItemDisplayStr(DocElectronicSignatureCertificate), MenuItemType::Display).create(args);
                    formRun.run();
                    formRun.wait(true);
                }
                finally
                {
                    appl.setDefaultCompany(curCompanyId, false);
                }
            }

]]></Source>
