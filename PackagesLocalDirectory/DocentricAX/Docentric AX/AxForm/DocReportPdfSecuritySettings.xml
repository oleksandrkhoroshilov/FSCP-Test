						<Source><![CDATA[
        /// <summary>
        /// Called when a record becomes active.
        /// </summary>
        /// <returns>True/False</returns>
        public int active()
        {
            int ret;
        
            ret = super();
        
            DocPdfEncryptionSettings docPdfEncryptionSettingsLocal;

            utcdatetime dateFrom = DateTimeUtil::minValue();
            utcdatetime dateTo = DateTimeUtil::maxValue();

            // Count the records to check if the selected record has a history of changes
            select validtimestate(dateFrom, dateTo) count(RecId) 
                from docPdfEncryptionSettingsLocal
                where docPdfEncryptionSettingsLocal.ReportTable == DocPdfEncryptionSettings.ReportTable
                   && docPdfEncryptionSettingsLocal.CompanyId == DocPdfEncryptionSettings.CompanyId
                   && docPdfEncryptionSettingsLocal.Password == conNull();

            // The Delete button must be enabled only on record creation or if the selected record 
            // does not have a history of changes and the password is empty
            DeleteEncryptionRule.enabled(!DocPdfEncryptionSettings.RecId || docPdfEncryptionSettingsLocal.RecId == 1);

            return ret;
        }

]]></Source>
