								<Name>lookup</Name>
								<Source><![CDATA[
            /// <summary>
            /// Calls lookup control.
            /// </summary>
            /// <param name = "_formControl">Form control</param>
            /// <param name = "_filterStr">Filter string</param>
            public void lookup(FormControl _formControl, str _filterStr)
            {
                if (!DocReportPdfSignatureSettings.CompanyId)
                {
                    DocGlobalHelper::handleWarning("@DocentricAX2:SelectCompanyFirst");
                }

                SysTableLookup sysTableLookup = SysTableLookup::newParameters(tableNum(DocElectronicSignatureCertificate), _formControl);

                Query query = new Query();
                query.allowCrossCompany(true);
                query.addCompanyRange(DocReportPdfSignatureSettings.CompanyId);
                query.addDataSource(tableNum(DocElectronicSignatureCertificate));

                sysTableLookup.addLookupfield(fieldNum(DocElectronicSignatureCertificate, CertificateId));
                sysTableLookup.addLookupfield(fieldNum(DocElectronicSignatureCertificate, CertificateSubject));
                sysTableLookup.addLookupfield(fieldNum(DocElectronicSignatureCertificate, ValidTo));
                sysTableLookup.addLookupfield(fieldNum(DocElectronicSignatureCertificate, CertificateStorage));
                sysTableLookup.addLookupfield(fieldNum(DocElectronicSignatureCertificate, ValidationStatus));

                sysTableLookup.parmQuery(query);

                sysTableLookup.performFormLookup();
            }

]]></Source>
