        /// <summary>
        /// Called when the TestConnections button is clicked.
        /// </summary>
        public void clicked()
        {
            // Executes a class static method in the client async sandbox.
            // Test connection for all storage accounts. 
            SysOperationSandbox::callStaticMethod(
                                    classNum(DocAzureHelper),
                                    staticMethodStr(DocAzureHelper, testAzureStorageAccountsConnections),
                                    conNull(),
                                    "@DocentricAX5:DocAzureStorageAccountsRefreshProcess",
                                    "@DocentricAX5:DocAzureStorageAccountsRefreshProcessCompleted");
            DocAzureStorageAccount_ds.research(true);
        }

]]></Source>
