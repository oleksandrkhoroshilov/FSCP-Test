					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        /// <summary>
        /// Calls lookup control.
        /// </summary>
        public void lookup()
        {
            if (!DocAzureStorageAccount.CompanyId)
            {
                DocGlobalHelper::handleWarning("@DocentricAX2:SelectCompanyFirst");
            }

            SysTableLookup sysTableLookup = SysTableLookup::newParameters(tableNum(KeyVaultCertificateTable), this);

            Query query = new Query();
            query.allowCrossCompany(true);
            query.addCompanyRange(DocAzureStorageAccount.CompanyId);
            query.addDataSource(tableNum(KeyVaultCertificateTable));

            sysTableLookup.addLookupfield(fieldNum(KeyVaultCertificateTable, Name));
            sysTableLookup.addLookupfield(fieldNum(KeyVaultCertificateTable, Description));
            sysTableLookup.addLookupfield(fieldNum(KeyVaultCertificateTable, KeyVaultRef));

            sysTableLookup.parmQuery(query);

            sysTableLookup.performFormLookup();
        }

]]></Source>
					</Method>
					<Method>
						<Name>jumpRef</Name>
						<Source><![CDATA[
        /// <summary>
        /// Jumps to the referenced table.
        /// </summary>
        public void jumpRef()
        {
            if (!DocAzureStorageAccount.CompanyId)
            {
                DocGlobalHelper::handleWarning("@DocentricAX2:SelectCompanyFirst");
            }

            CompanyId curCompanyId = curExt();
            appl.setDefaultCompany(DocAzureStorageAccount.CompanyId, false);

            try
            {
                Args args = new Args();
                args.lookupField(fieldNum(KeyVaultCertificateTable, RecId));
                args.lookupValue(int642Str(DocAzureStorageAccount.KeyVaultCertificateRef));
                args.caller(element);

                FormRun formRun = new MenuFunction(menuItemDisplayStr(KeyVaultParameters), MenuItemType::Display).create(args);
                formRun.run();
                formRun.wait(true);
            }
            finally
            {
                appl.setDefaultCompany(curCompanyId, false);
            }
        }

]]></Source>
