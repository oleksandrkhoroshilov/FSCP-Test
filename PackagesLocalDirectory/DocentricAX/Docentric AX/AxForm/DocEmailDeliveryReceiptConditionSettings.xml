					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret;
        
            ret = super();

            if (DocReportCompanySetting_EmailDeliveryReceiptCondition.Report != DocReportTable.RecId)
            {
                DocGlobalHelper::handleError('Inconsistent data - relation to the underlaying report is lost');
                ret = false;
            }
            
            
            if (DocReportCompanySetting_EmailDeliveryReceiptCondition.SettingName != DocConstant::EmailDeliveryReceiptConditionSettingName)
            {
                DocGlobalHelper::handleError(strFmt('Inconsistent data - DocReportCompanySetting.SettingName (%1) differs from the valid value (%2)',
                                                    DocReportCompanySetting_EmailDeliveryReceiptCondition.SettingName, DocConstant::EmailDeliveryReceiptConditionSettingName));
                ret = false;
            }
        
            return ret;
        }

]]></Source>
