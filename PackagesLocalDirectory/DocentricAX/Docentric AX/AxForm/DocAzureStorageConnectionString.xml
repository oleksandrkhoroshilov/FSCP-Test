{
}
]]></Source>
			</Method>
			<Method>
				<Name>getGeneratedConnectionString</Name>
				<Source><![CDATA[
    /// <summary>
    /// Generates role based access connection string.
    /// </summary>
    /// <returns>Role based access connection string.</returns>
    private str getGeneratedConnectionString()
    {
        DocStorageAccountConnectionStringSettings connectionStringSettings = new DocStorageAccountConnectionStringSettings();

        connectionStringSettings.StorageAccountName = AzureStorageAccountName.text();
        connectionStringSettings.EndpointSuffix =  EndpointSuffix.text();
        connectionStringSettings.TenantId = AzureTenantId.text();
        connectionStringSettings.ApplicationId = AzureApplicationId.text();
        connectionStringSettings.ApplicationSecret = AzureApplicationSecret.text();

        return DocAzureHelper::generateStorageAccountConnectionString(connectionStringSettings, true);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Dialog initialization.
    /// </summary>
    public void init()
    {
        super();
        
        // On a form pattern is applied and sets buttonDisplay property to ImageOnly
        // therefore, we have to change it back to Auto in order to show button caption text
        GenerateConnectionString.buttonDisplay(ButtonDisplay::Auto);
        GenerateConnectionString.extendedStyle('None');

        // Set default value for endpoint suffix
        EndpointSuffix.text(DocConstant::AzureStorageAccountEndpointSuffix);
    }

]]></Source>
			</Method>
